{
  "lspconfig": {
    "rust_analyzer": {
      "rust-analyzer.cargo.buildScripts.overrideCommand": [
        "cargo",
        "+esp",
        "check",
        "--quiet",
        "--workspace",
        "--message-format=json",
        "--all-targets"
      ],
      "rust-analyzer.cargo.extraEnv": {
        "CRATE_CC_NO_DEFAULTS":"1"
      },
      "rust-analyzer.checkOnSave": true
    }
  }
}
